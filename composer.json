{
    "name": "alephit/bitbucket-webhook-package",
    "description": "Bitbucket webhook package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleph IT Dev Team",
            "email": "support@alephit.com.au"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/console": "^v8.16.1",
        "illuminate/support": "^v8.16.1"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpunit/phpunit": "^9.4.3"
    },
    "autoload": {
        "psr-4": {
            "AlephIt\\BitbucketWebhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlephIt\\BitbucketWebhookTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlephIt\\BitbucketWebhook\\BitbucketWebhookServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
